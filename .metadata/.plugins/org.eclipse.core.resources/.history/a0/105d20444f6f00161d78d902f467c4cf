   <!--  
      window.onload 실행시 btnGetData 요소에 onclick 걸고
      txtmessage  요소 정보를 얻어서
      id 속성의 값(getAtt..
      value 속성의 값(getAtt..
      실제 value 값하고 (요소.value)
           value 값의 길이도 같이 출력하세요
      출력하고 (console.log)
      
   		btnCreate  요소에 onclick 이벤트 걸고
      input 태그 동적으로 생성하고
      type=text , id=txtbox , value=홍길동 이라 정의하고
      body 자식요소로 추가하세요
   -->
   <script type="text/javascript">
   	window.onload = function(){
   		var GetData = document.getElementById("btnGetData");
   		var Create = document.getElementById("btnCreate");
   		
   		GetData.onclick = function(){
   	/* 	 id 속성의 값(getAtt..
   		      value 속성의 값(getAtt..
   		      실제 value 값하고 (요소.value)
   		           value 값의 길이도 같이 출력하세요
   		      출력하고 (console.log) */
   		      console.log( GetData.nodeName+"/"+GetData.nodeType +"/"+GetData.nodeValue +"/"+GetData.value);
   		      
   		}
   		
   		Create.onclick = function(){
/*    		   input 태그 동적으로 생성하고
   	      type=text , id=txtbox , value=홍길동 이라 정의하고
   	      body 자식요소로 추가하세요 */
   		}
   		
   	}
   </script>
