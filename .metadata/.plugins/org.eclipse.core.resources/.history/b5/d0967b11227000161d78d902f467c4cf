<script type="text/javascript">
   var flag = true;
   var flag2 = true;
   var flag3 = true;
   var flag4 = true;
   var flag5 = true;
   var flag6 = true;
   var flag7 = true;
   
   window.onload = function() {
      var video1 = document.getElementById("video1");
      var video2 = document.getElementById("video2");
      var video3 = document.getElementById("video3");
      var video4 = document.getElementById("video4");
      var video5 = document.getElementById("video5");
      var video6 = document.getElementById("video6");
      var video7 = document.getElementById("video7");
         

      //autoplay
      video1.onclick = function() {
         var div = document.getElementById("div1");
         if(flag){
         var video = document.createElement("video");
         var source = document.createElement("source");
         var br = document.createElement("br");
         var br2 = document.createElement("br");
         var img = document.createElement("img");
         video.setAttribute("controls", "controls");
         video.setAttribute("autoplay", "autoplay");
         video.setAttribute("width", "360");
         video.setAttribute("height", "240");
         source.setAttribute("src", "../videos/Google Maps- Pok√©mon Challenge.mp4");
         source.setAttribute("type", "video/mp4");
         img.setAttribute("src", "../images/autoplay.GIF");
         div.appendChild(br2);
         div.appendChild(video).appendChild(source);
         div.appendChild(br);
         div.appendChild(img);
         flag = false;
         }else{
         if (div.style.display == "block") {
            div.style.display = "none";
         } else {
            div.style.display = "block";
            
         }
         }
         
         
      }
      
        //loop
         video2.onclick = function() {
            var div = document.getElementById("div2");
            if(flag2){
            var video = document.createElement("video");
            var source = document.createElement("source");
            var br = document.createElement("br");
            var br2 = document.createElement("br");
            var img = document.createElement("img");
            video.setAttribute("controls", "controls");
            video.setAttribute("loop", "loop");
            video.setAttribute("width", "360");
            video.setAttribute("height", "240");
            source.setAttribute("src", "../videos/loop.mp4");
            source.setAttribute("type", "video/mp4");
            img.setAttribute("src", "../images/loop.GIF");
            div.appendChild(br2);
            div.appendChild(video).appendChild(source);
            div.appendChild(br);
            div.appendChild(img);
            flag2 = false;
            }else{
            if (div.style.display == "block") {
               div.style.display = "none";
            } else {
               div.style.display = "block";

            }
            }
         }
         
         //poster
         video3.onclick = function() {
            var div = document.getElementById("div3");
            if(flag3){
            var video = document.createElement("video");
            var source = document.createElement("source");
            var br = document.createElement("br");
            var br2 = document.createElement("br");
            var img = document.createElement("img");
            video.setAttribute("controls", "controls");
