                div.appendChild(video).appendChild(source);
                div.appendChild(br);
                div.appendChild(img);
                flag5 = false;
                }else{
                if (div.style.display == "block") {
                   div.style.display = "none";
                } else {
                   div.style.display = "block";
                }
                }
            }
            //preload-none-controls
               video6.onclick = function() {
                var div = document.getElementById("div6");
                if(flag6){
                var video = document.createElement("video");
                var source = document.createElement("source");
                var br = document.createElement("br");
                var br2 = document.createElement("br");
                var img = document.createElement("img");
                video.setAttribute("controls", "controls"); 
                video.setAttribute("preload", "none");
                video.setAttribute("width", "360");
                video.setAttribute("height", "240");
                source.setAttribute("src", "../videos/Pokemon2.mp4");
                source.setAttribute("type", "video/mp4");
                img.setAttribute("src", "../images/none.GIF");
                div.appendChild(br2);
                div.appendChild(video).appendChild(source);
                div.appendChild(br);
                div.appendChild(img);
                flag6 = false;
                }else{
                if(div.style.display == "block") {
                   div.style.display = "none";
                } else {
                    div.style.display = "block";
                  }
                }
            }
                        
            //muted
                    video7.onclick = function() {
                        var div = document.getElementById("div7");
                        if(flag7){
                        var video = document.createElement("video");
                        var source = document.createElement("source");
                        var br = document.createElement("br");
                        var br2 = document.createElement("br");
                        var img = document.createElement("img");
                        video.setAttribute("controls", "controls"); 
                        video.setAttribute("preload", "metadata");
                        video.setAttribute("muted","muted");
