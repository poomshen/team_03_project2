
public class MemberDAO {
	
	private String Id;
	private String name;
	private String pass;
	
	
	public void setMember(String id , String name , String pass ){
		this.Id = id;
		this.name = name;
		this.pass = pass;
	}
	public MemberDAO getMember(){
		return this;
	}
	
	public String getId() {
		return Id;
	}
	public void setId(String id) {
		Id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getPass() {
		return pass;
	}
	public void setPass(String pass) {
		this.pass = pass;
	}

	
}
